// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model News {
  id        Int       @id @default(autoincrement())
  category  String
  title     String
  date      String
  author    String
  pTags     String
  imgLinks  String
  contentEn String
  ref       String
  titleTh   String
  contentTh String
}